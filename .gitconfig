# This is Git's per-user configuration file.
[user]
	name = Stephan Zahariev
	email = 23424829+StephanZahariev@users.noreply.github.com

[core]
	excludesfile = /Users/stephan.zahariev/.gitignore

[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process

[alias]
	logg = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(auto)%d%C(reset)'

[url "git@github.com:"]
	insteadOf = https://github.com/
